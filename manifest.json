{
  "name": "BlockContent",
  "description": "Blocks adult content",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage"]
}
